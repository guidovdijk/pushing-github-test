{
  "name": "the-valley",
  "version": "1.0.0",
  "description": "custom eslint",
  "main": "index.js",
  "dependencies": {
    "eslint": "^5.7.0",
    "stylelint-scss": "^3.3.1"
  },
  "devDependencies": {
    "browser-sync": "^2.26.3",
    "del": "^3.0.0",
    "eslint": "^5.7.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cache": "^1.0.2",
    "gulp-clean-css": "^3.10.0",
    "gulp-eslint": "^5.0.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^4.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-stylelint": "^7.0.0",
    "gulp-uglify": "^3.0.1",
    "gulp-useref": "^3.1.5",
    "husky": "^1.1.2",
    "lint-staged": "^7.3.0",
    "postcss-reporter": "^6.0.0",
    "run-sequence": "^2.2.1",
    "stylelint": "^9.6.0"
  },
  "scripts": {
    "lint": "./node_modules/.bin/eslint --fix src/assets/scripts/**/*.js --max-warnings=0",
    "stylelint": "./node_modules/.bin/stylelint --fix src/assets/styles/**/*.scss --max-warnings=0",
    "test": "gulp commit"
  },
  "lint-staged": {
    "js": "npm run lint -s",
    "scss": "npm run stylelint -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "author": "Guido",
  "license": "ISC"
}
