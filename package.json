  "scripts": {
    "precommit": "lint-staged",
    "prod": "gulp production --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidovdijk/pushing-github-test.git"
  },
  "lint-staged": {
    "src/assets/scripts/**/*.js": "./node_modules/.bin/eslint --fix",
